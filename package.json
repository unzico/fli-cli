{
    "name": "crew",
    "version": "0.1.0",
    "license": "MIT",
    "private": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/unzico/crew.git"
    },
    "bugs": {
        "url": "https://github.com/unzico/crew/issues"
    },
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "crew": "./dist/cli.js"
    },
    "files": [
        "dist"
    ],
    "exports": {
        ".": "./dist/index.js"
    },
    "scripts": {
        "test": "bun run build && bun test",
        "build": "bun build ./src/index.ts ./src/cli.ts --outdir ./dist --target bun && tsc -p tsconfig.build.json",
        "format": "prettier --write .",
        "release": "bun run scripts/release.ts"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "prettier": "3.7.4"
    },
    "peerDependencies": {
        "typescript": "5.9.3",
        "commander": "^14"
    }
}
