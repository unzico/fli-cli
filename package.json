{
    "name": "fli-cli",
    "version": "0.0.2",
    "private": false,
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "fli-cli": "./dist/cli.js"
    },
    "files": [
        "dist"
    ],
    "exports": {
        ".": "./dist/index.js"
    },
    "scripts": {
        "build": "bun build ./src/index.ts ./src/cli.ts --outdir ./dist --target bun && tsc -p tsconfig.build.json",
        "format": "prettier --write .",
        "release": "bun run scripts/release.ts"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "prettier": "3.7.4"
    },
    "peerDependencies": {
        "typescript": "5.9.3"
    },
    "dependencies": {
        "commander": "14.0.2"
    }
}
