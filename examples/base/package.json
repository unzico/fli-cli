{
    "name": "mycli-example",
    "module": "index.ts",
    "type": "module",
    "dependencies": {
        "fli-cli": "file:../../"
    },
    "scripts": {
        "build": "bun run ../../dist/cli.js build ./index.ts",
        "build:cli": "bun run ../../dist/cli.js build --name cli --baseDir src",
        "watch": "bun run ../../dist/cli.js watch src/**/*.ts ./index.ts",
        "watch:cli": "bun run ../../dist/cli.js watch src/**/*.ts --name cli --baseDir src"
    }
}
